{
  "name": "api_memlane_v3",
  "version": "1.2.0",
  "description": "A node api made for the memorylane app in vueJS",
  "main": "server.js",
  "scripts": {
    "test": "test",
    "start": "nodemon server.js",
    "build-db": "docker run --name memorylane-postgres -p 5432:5432 -e POSTGRES_PASSWORD=postgres -e POSTGRES_USER=admin -d postgres",
    "init-db": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "prep-db": "sequelize db:migrate && sequelize db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OwenCoogan/api_memlane_v3.git"
  },
  "author": "Owen Coogan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OwenCoogan/api_memlane_v3/issues"
  },
  "homepage": "https://github.com/OwenCoogan/api_memlane_v3#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.18.0",
    "sequelize-cli": "^6.4.1"
  }
}
