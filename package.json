{
  "name": "api_memlane_v3",
  "version": "2.2.0",
  "description": "A node api made for the memorylane app in vueJS",
  "main": "server.js",
  "scripts": {
    "test": "test",
    "start": "nodemon server.js",
    "build-db": "docker compose up",
    "build-image": "docker build -t memorylane-node-js .",
    "init-db": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "prep-db": "sequelize db:migrate && sequelize db:seed:all",
    "prep-db-prod": "sequelize db:migrate --env production && sequelize db:seed:all",
    "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OwenCoogan/api_memlane_v3.git"
  },
  "author": "Owen Coogan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/OwenCoogan/api_memlane_v3/issues"
  },
  "homepage": "https://github.com/OwenCoogan/api_memlane_v3#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.18.0",
    "sequelize-auto-migrations": "^1.0.3",
    "sequelize-cli": "^6.4.1"
  }
}
